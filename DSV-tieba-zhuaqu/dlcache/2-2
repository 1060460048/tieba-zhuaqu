<script>    _.Module.use('tbui/widget/httpTransform', [], function () {
    });
    _.Module.use('search/widget/suggestion', [], function () {
    });
    _.Module.use('search/widget/search_logic', [
        $('#head'),
        {
            product: 'frs',
            forumName: '成都信息工程学院',
            searchFixed: '1',
            sugOn: '1'
        }
    ]);
